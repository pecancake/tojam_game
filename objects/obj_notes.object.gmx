            <kind>1</kind>
            <string>/// Pattern Sequencer

if !close
{
    if image_xscale!=200
    {
        image_xscale+=8
        x-=4
        image_alpha+=0.04
        
        if image_xscale=200
        {
            instance_create(x,520,obj_hitbar)
            
            with(obj_notes_pip)
            {
                vspeed=-5
            }
        }
    }
}
else
