            <string>/// Initialize
pattern=create_pattern(irandom_range(2,5))
tick_rate=obj_host.tick_rate

counter=0
seek=0

image_alpha=0

close=false
image_xscale=0

while seek!=string_length(pattern)
{
    for (i=0;i!=4;i+=1)
    {
        if string_char_at(pattern,seek+i+1)='1'
        {
            pip=instance_create(225+50*i,320-counter*room_speed/tick_rate,obj_notes_pip)
            pip.vspeed=room_speed/tick_rate
            pip.original_vspeed=pip.vspeed
